        <id column="site_id" property="siteId"/>
        <result column="site_name" property="siteName"/>
        <result column="site_category" property="siteCategory"/>
        <result column="site_description" property="siteDescription"/>
        <result column="is_active" property="isActive"/>
        <result column="sort_order" property="sortOrder"/>
        <result column="created_time" property="createdTime"/>
        <result column="updated_time" property="updatedTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        site_id, site_name, site_category, site_description, is_active, sort_order, 
        created_time, updated_time
    </sql>

</mapper> 